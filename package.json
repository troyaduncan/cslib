    "xmlrpc": "^1.3.2",
    "fast-xml-parser": "^4.3.2",
    "ssh2": "^1.15.0",
    "dns": "^0.2.2",
    "dns-packet": "^5.6.1",
    "dotenv": "^16.3.1",
    "reflect-metadata": "^0.2.1",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
